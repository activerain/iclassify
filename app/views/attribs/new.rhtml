<fieldset>
  <legend>Add an Attribute</legend>
<%= error_messages_for :attrib %>
<% form_for :attrib, :url => node_attribs_path(:node_id => @node.id) do |form| %>
<%= render :partial => 'form', :locals => { :node => @node, :form => form }  %>
<%= submit_tag "Add Attribute" %>

<% end %>
</fieldset>
